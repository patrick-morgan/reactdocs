{
    "name": "reactdocs",
    "displayName": "React Docs",
    "description": "Easily view React documentation in VS Code",
    "version": "1.0.0",
    "publisher": "PatrickMorgan",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "keywords": [
      "react", "docs", "documentation", "javascript", "react documentation"
    ],
    "icon": "images/react-logo.png",
    "license": "SEE LICENSE IN LICENSE.TXT",
    "main": "./dist/extension.js",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "type": "webview",
                    "id": "reactdocs.documentation",
                    "name": "Documentation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack --config ./build/node-extension.webpack.config.js",
        "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
        "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "npm run test-compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/patrick-morgan/reactdocs"
    },
    "bugs": {
      "url": "https://github.com/patrick-morgan/reactdocs/issues"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/remarkable": "^2.0.0",
        "@types/vscode": "^1.52.0",
        "@types/whatwg-fetch": "0.0.33",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "highlight.js": "^10.4.1",
        "mdast-util-from-markdown": "^0.8.4",
        "mdast-util-to-markdown": "^0.6.2",
        "remarkable": "^2.0.1",
        "showdown": "^1.9.1"
    }
}
